# Appwrite Configuration Example
# Copy this file to .env and fill in your actual values

# Your Appwrite endpoint (e.g., https://cloud.appwrite.io/v1)
VITE_APPWRITE_ENDPOINT=your_appwrite_endpoint_here

# Your Appwrite project ID
VITE_APPWRITE_PROJECT_ID=your_project_id_here

# Your Appwrite database ID
VITE_APPWRITE_DATABASE_ID=your_database_id_here

# Collection IDs (you can keep the defaults or change them to match your setup)
VITE_APPWRITE_USERS_COLLECTION=users
VITE_APPWRITE_SESSIONS_COLLECTION=sessions
VITE_APPWRITE_MFA_COLLECTION=mfa_verifications

# Your Appwrite API key (for server-side operations)
# WARNING: Never commit your actual API key to version control
VITE_APPWRITE_API_KEY=your_api_key_here
